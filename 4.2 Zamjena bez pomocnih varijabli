#include<iostream>
using namespace std;

//Bez koristenja pomocne varijable i funkcije swap

int main(){
	int a,b;
	cout << "Unesite a: ";
	cin >> a;
	cout << "Unesite b: ";
	cin >> b;
	cout << "Brojevi su: " << a << " i " << b << ".\n";
	
	a=a+b;  //nova vrijednost od a je njihov zbir  a=5, b=4    a=5+4
	b=a-b;  //b uzima vrijednost od a                          b=9-4...=5
	a=a-b;  //koristi se nova vrijednost od b                  a=9-5...=4
	
	
	cout << "Brojevi nakon zamjene su: " << a 
	     << " i " << b << ".\n";
	 }
